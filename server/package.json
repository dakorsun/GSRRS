    "fs-extra": "^7.0.0",
    "jest": "^24.8.0",
    "node-sass-chokidar": "^1.3.5",
    "number-to-words": "^1.2.4",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "resolve-url-loader": "^2.3.0",
    "sequelize-cli": "^5.5.0",
