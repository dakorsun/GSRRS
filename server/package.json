{
  "name": "GSRRS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "server": "cross-env NODE_ENV=development node -r esm ./src/index.js",
    "debug": "cross-env NODE_ENV=development node --inspect -r esm ./src/index.js",
    "server:prod": "cross-env NODE_ENV=production node -r esm ./src/index.js",
    "seq-sync": "node -r esm ./config/database/syncDatabase.js",
    "seq-migrations": "sequelize db:migrate",
    "seed": "cross-env NODE_ENV=development node -r esm config/database/seeds/execSeed",
    "seed:prod": "cross-env NODE_ENV=production node -r esm config/database/seeds/execSeed",
    "script:whole_cup": "cross-env NODE_ENV=development DB_SCRYPT_TYPE=WHOLE_CUP node -r esm config/database/scripts",
    "drop": "node -r esm ./config/database/drop.js"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "axios": "^0.18.1",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.1",
    "body-parser": "^1.18.3",
    "boom": "^7.2.0",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "ejs": "^2.7.2",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "express-promise-router": "^3.0.3",
    "express-redis-cache": "^1.1.3",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "latinize": "^0.4.0",
    "lodash": "^4.17.10",
    "merge-stream": "^1.0.1",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "mysql2": "^1.6.5",
    "node-schedule": "^1.3.2",
    "query-string": "^5.1.1",
    "redis": "^2.8.0",
    "request-ip": "^2.1.3",
    "rpi-gpio": "^2.1.5",
    "rpi-gpio-buttons": "^1.1.1",
    "sequelize": "^5.9.1",
